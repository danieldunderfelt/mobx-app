{
  "name": "mobx-app",
  "version": "0.0.2",
  "description": "MobX, with a functional structure.",
  "repository": {
    "type": "git",
    "url": "git://github.com/danieldunderfelt/mobx-app.git"
  },
  "main": "src/index",
  "scripts": {
    "build": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "preversion": "npm run build"
  },
  "babel": {
    "presets": [
      "stage-2",
      "es2015"
    ]
  },
  "keywords": [
    "mobx",
    "functional",
    "mobx-app",
    "structure"
  ],
  "author": "Daniel Dunderfelt <dunderfeltdaniel@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "lodash": ">=4",
    "mobx": ">=2",
    "mobx-react": ">=3"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0"
  }
}
